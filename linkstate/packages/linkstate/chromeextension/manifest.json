{
  "name": "Youiest Make Connections",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "You make the connections",
  "homepage_url": "http://youiest.com",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": ["storage", "activeTab", "tabs", "http://*/*" ],
  "chrome_url_overrides": {
    "newtab": "src/newtab/newtab.html"
  },
  "background": {
    "scripts": [
      "src/background.js"
    ]
  }
}
